<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
                    xmlns:behavior1="clr-namespace:PeletonSoft.Tools.View.Behavior;assembly=Tools.View"
                    xmlns:present11="clr-namespace:PeletonSoft.Sketch.ViewModel.Present;assembly=Sketch.ViewModel">
    
    <DataTemplate DataType="{x:Type present11:PreviewPresentViewModel}">
		<GroupBox Header="Презентация">
			<StackPanel Orientation="Horizontal">
				<Button Content="Изображение..." Width="110" Height="25">
					<i:Interaction.Behaviors>
						<behavior1:OpenImageFileBehavior Command="{Binding OpenFileCommand}"/>
					</i:Interaction.Behaviors>
				</Button>
			</StackPanel>
		</GroupBox>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type present11:LayoutPresentViewModel}"/>
    
</ResourceDictionary>