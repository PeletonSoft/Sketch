<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:PeletonSoft.Sketch.ViewModel.WorkMode;assembly=Sketch.ViewModel"
                    xmlns:workMode="clr-namespace:PeletonSoft.Sketch.View.Controls.WorkMode">

    <DataTemplate DataType="{x:Type vm:EditorWorkModeViewModel}">
        <workMode:EditorWorkModeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ReportWorkModeViewModel}">
        <workMode:ReportWorkModeView/>
    </DataTemplate>
    
</ResourceDictionary>