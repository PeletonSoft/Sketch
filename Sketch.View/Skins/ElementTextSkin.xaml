<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converter="clr-namespace:PeletonSoft.Tools.View.Converter;assembly=Tools.View"
                    xmlns:element="clr-namespace:PeletonSoft.Sketch.ViewModel.Element;assembly=Sketch.ViewModel"
                    xmlns:custom1="clr-namespace:PeletonSoft.Sketch.ViewModel.Element.Custom;assembly=Sketch.ViewModel"
                    xmlns:text="clr-namespace:PeletonSoft.Sketch.View.Controls.Element.Text">

    <converter:TypeToListDictionaryItemConverter x:Key="TypeToListDictionaryItemConverter"/>
    
    <DataTemplate DataType="{x:Type custom1:AlignableElementViewModel}">
        <text:ElementTextView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type element:OverlayViewModel}">
        <text:ElementTextView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type element:TieBackViewModel}">
        <text:ElementTextView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type element:PleatViewModel}">
        <text:ElementTextView/>
    </DataTemplate>
    
</ResourceDictionary>